<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">

<fontscolors>
	<fontcolor name="attribute-name" foreColor="ff007c00" default="default"/>
	<fontcolor name="attribute-value" foreColor="ff99006b" default="default"/>
	<fontcolor name="comment" foreColor="gray" default="default">
		<font style="bold"/>
	</fontcolor>
	<fontcolor name="error" default="error">
		<font style="bold"/>
	</fontcolor>
	<fontcolor name="scriptlet-delimiter" foreColor="gray" default="default">
		<font style="bold"/>
	</fontcolor>
	<fontcolor name="symbol" foreColor="gray" default="default">
		<font style="bold"/>
	</fontcolor>
	<fontcolor name="tag-directive" foreColor="ff999900" default="default">
		<font style="bold"/>
	</fontcolor>
	<fontcolor name="endtag" foreColor="ff999900" default="default">
		<font style="bold"/>
	</fontcolor>
	<fontcolor name="jsp-whitespace" foreColor="black" default="default">
		<font style="bold"/>
	</fontcolor>
	<fontcolor name="scriptlet" default="default"/>
	<fontcolor name="expression-language" foreColor="yellow" default="default"/>
</fontscolors>
