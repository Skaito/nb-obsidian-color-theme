<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">

<fontscolors>
	<!-- JavaTokenId categories -->
	<fontcolor name="character" default="char"/>
	<fontcolor name="errors" default="error"/>
	<fontcolor name="identifier" default="identifier"/>
	<fontcolor name="keyword" default="keyword"/>
	<fontcolor name="keyword-directive" default="keyword"/>
	<fontcolor name="literal" default="keyword" />
	<fontcolor name="comment" default="comment"/>
	<fontcolor name="number" default="number"/>
	<fontcolor name="operator" default="operator"/>
	<fontcolor name="string" default="string"/>
	<fontcolor name="separator" default="separator"/>
	<fontcolor name="whitespace" default="whitespace"/>
    
	<!--"semantic" highlighting:-->
    
	<fontcolor name="mod-constructor-declaration" default="constructor" />
	<fontcolor name="mod-constructor" default="constructor" />
	<fontcolor name="mod-method-declaration" default="method" />
	<fontcolor name="mod-method" default="method" />
	<fontcolor name="mod-field-declaration" foreColor="098A18" default="field" />
	<fontcolor name="mod-field" foreColor="098A18" default="field" />
	<fontcolor name="mod-local-variable-declaration" default="local-variable" />
	<fontcolor name="mod-local-variable" default="local-variable" />
	<fontcolor name="mod-parameter-declaration" foreColor="ec8e01" default="method-parameter" />
	<fontcolor name="mod-parameter" foreColor="ec8e01" default="method-parameter" />

	<fontcolor name="mod-class-declaration" default="class"/>
	<fontcolor name="mod-class" default="class" />
	<fontcolor name="mod-interface-declaration" default="interface"/>
	<fontcolor name="mod-interface" default="interface" />
	<fontcolor name="mod-annotation-type-declaration" default="interface"/>
	<fontcolor name="mod-annotation-type" default="interface" />
	<fontcolor name="mod-enum-declaration" default="enum"/>
	<fontcolor name="mod-enum" default="enum" />

	<fontcolor name="mod-unused" foreColor="gray" default="unused" />
	<fontcolor name="mod-deprecated" default="deprecated" />
	<fontcolor name="mod-abstract" default="abstract" />
	<fontcolor name="mod-static" default="static" />
	<fontcolor name="mod-private" default="private" />
	<fontcolor name="mod-package-private" default="private"/>
	<fontcolor name="mod-protected" default="protected" />
	<fontcolor name="mod-public" default="public" />
    
	<!--currently not used:-->
	<!--    <fontcolor name="mod-type-parameter-declaration" bgColor="lightGray"/>
	<fontcolor name="mod-type-parameter-use" bgColor="green"/>
	<fontcolor name="mod-undefined" foreColor="red"/>-->

	<fontcolor name="mark-occurrences" bgColor="38566f"/>

	<!-- JavaStringTokenId categories -->
	<fontcolor name="string-escape" default="string">
		<font style="bold" />
	</fontcolor>
	<fontcolor name="string-escape-invalid" default="string">
		<font style="italic" />
	</fontcolor>
    
	<!-- JavaCharacterTokenId categories -->
	<fontcolor name="character-escape" default="character">
		<font style="bold" />
	</fontcolor>
	<fontcolor name="character-escape-invalid" default="character">
		<font style="italic" />
	</fontcolor>
   
	<!-- JavadocTokenId categories -->
	<fontcolor name="javadoc-tag" default="doc-comment-tag">
		<font style="bold" />
	</fontcolor>
	<fontcolor name="javadoc-identifier" default="doc-comment-identifier"/>
	<fontcolor name="javadoc-first-sentence" default="comment"/>
    
	<fontcolor name="html-tag" foreColor="428BDD" default="default"/>
</fontscolors>
