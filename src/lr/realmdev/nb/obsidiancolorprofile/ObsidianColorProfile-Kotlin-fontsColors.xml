<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">

<fontscolors>
	<fontcolor name="mark-occurrences" default="mark-occurrences" />
	<fontcolor name="KEYWORD" default="keyword" />
	<fontcolor name="WHITESPACE" default="whitespace" />
	<fontcolor name="IDENTIFIER" default="identifier" />
	<fontcolor name="STRING" default="string" />
	<fontcolor name="ANNOTATION" default="interface" />
	<fontcolor name="SINGLE_LINE_COMMENT" default="comment" />
	<fontcolor name="MULTI_LINE_COMMENT" default="comment" />
	<fontcolor name="KDOC_TAG_NAME" default="doc-comment-identifier" />
	<fontcolor name="KDOC_LINK" default="doc-comment-tag" />
	
	<fontcolor name="mod-field" default="field" />
    <fontcolor name="mod-class" default="class" />
    <fontcolor name="mod-interface" default="interface" />
	<fontcolor name="mod-enum" default="enum" />
    <fontcolor name="mod-parameter" default="method-parameter" />
    <fontcolor name="mod-static" default="static" />
    <fontcolor name="mod-custom1" />
    <fontcolor name="mod-custom2" />
    <fontcolor name="mod-deprecated" default="deprecated" />
    <fontcolor name="mod-annotation-type" default="interface" />
    <fontcolor name="mod-declaration" default="identifier">
        <font style="bold" />
    </fontcolor>
    <fontcolor name="mod-type-parameter-use" default="identifier" />
    <fontcolor name="mod-local-variable" default="local-variable" />
